  "license": "MIT",
  "devDependencies": {
    "@types/d3": "^7.1.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/reactstrap": "^8.7.2",
    "browserify": "^17.0.0",
    "d3": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.2",
    "react-router-dom": "^6.2.2",
    "reactstrap": "^9.0.1",
    "tsify": "^5.0.4",
    "uglify-es": "^3.3.9",
    "watchify": "^4.0.0"
  }
